<div class="container-fluid">
  <div class="row mb-2">
    <div class="col-sm-6">
      <blockquote class="quote-{{ guild.color ?: 'info' }}"> 
        <h1>{{ title ?: page.title }}</h1>
      </blockquote>
    </div>
    <div class="col-sm-6">
      <ol class="breadcrumb float-sm-right">
          {% for item in page.breadcrumb %}
          <a href="{{ item.url }}">{{ item.name }}</a>
          {% endfor %}
      </ol>
    </div>
  </div>
</div><!-- /.container-fluid -->

